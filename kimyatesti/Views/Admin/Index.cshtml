@model IEnumerable<kimyatesti.identity.AppUser>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<div class="panel panel-primary">
    <div class="panel-heading">
        Kullanıcılar
    </div>
    <table class="table table-striped table-hover">
        <tr>
            <th>Id</th>
            <th>Ad</th>
            <th>Soyad</th>
            <th>Kullanıcı adı</th>
            <th>Email</th>
        </tr>
        @if (Model.Count() == 0)
        {
            <tr>
                <td colspan="3" class="text-center">
                    Kullanıcı yok
                </td>
            </tr>
        }
        else
        {
            foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                    <td>@item.Surname</td>
                    <td>@item.UserName</td>
                    <td>@item.Email</td>
                </tr>
            }
        }

    </table>

</div>